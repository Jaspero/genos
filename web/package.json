{
  "name": "web",
  "scripts": {
    "start": "npm run build:rollup && npm run dev:server",
    "build": "node ./src/cleanPublic.js && npm run build:rollup && npm run build:html && cp -r public/ ../public/website",
    "build:rollup": "rollup -c",
    "build:html": "node ./src/build.js",
    "build:html:incremental": "npm run build:html -- incremental && cp -r public/ ../public/website",
    "dev:rollup": "rollup  -c -w --no-watch.clearScreen",
    "dev:server": "nodemon",
    "eslint": "eslint --ext .js .",
    "eslint:fix": "eslint --fix --ext .js .",
    "prettier:svelte": "prettier --write --plugin-search-dir=. ./**/*.svelte"
  },
  "dependencies": {
    "@elderjs/elderjs": "1.2.2",
    "@elderjs/plugin-browser-reload": "0.1.20",
    "@elderjs/plugin-sitemap": "0.1.20",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "del": "5.1.0",
    "dotenv": "8.2.0",
    "firebase": "8.0.2",
    "fs-extra": "9.0.1",
    "glob": "7.1.6",
    "intersection-observer": "0.11.0",
    "polka": "0.5.2",
    "sirv": "1.0.7",
    "svelte": "3.29.7",
    "svelte-preprocess": "4.6.0",
    "systemjs": "6.7.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.12.1",
    "@babel/preset-env": "7.12.1",
    "@babel/runtime": "7.12.1",
    "@rollup/plugin-replace": "2.3.4",
    "autoprefixer": "9.8.6",
    "eslint": "7.12.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.1.4",
    "firebase-admin": "9.3.0",
    "node-sass": "4.14.1",
    "nodemon": "2.0.6",
    "prettier": "2.1.2",
    "prettier-plugin-svelte": "1.4.1",
    "rollup": "2.33.3",
    "rollup-plugin-terser": "7.0.2"
  },
  "engines": {
    "node": ">= 12.0.0"
  }
}
