{
  "name": "web",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "copy:dev": "node run.cjs dev",
    "copy:prod": "node run.cjs prod",
    "start:dev": "npm run copy:dev && vite dev",
    "start:prod": "npm run copy:prod && vite dev",
    "build:dev": "npm run copy:dev && vite build",
    "build:prod": "npm run copy:prod && vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:integration": "playwright test",
    "test:unit": "vitest"
  },
  "devDependencies": {
    "@playwright/test": "1.46.1",
    "@sveltejs/kit": "2.5.24",
    "@sveltejs/vite-plugin-svelte": "3.1.2",
    "@typescript-eslint/eslint-plugin": "8.2.0",
    "@typescript-eslint/parser": "8.2.0",
    "autoprefixer": "10.4.20",
    "eslint": "9.9.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-svelte": "2.43.0",
    "postcss": "8.4.41",
    "svelte": "5.0.0-next.236",
    "svelte-check": "3.8.6",
    "tslib": "2.7.0",
    "typescript": "5.5.4",
    "vite": "5.4.2",
    "vitest": "2.0.5"
  },
  "type": "module",
  "dependencies": {
    "@jaspero/utils": "1.15.3",
    "@jaspero/web-components": "1.153.2",
    "firebase": "10.13.1",
    "firebase-admin": "12.4.0"
  }
}
