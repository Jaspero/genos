{
  "name": "cms",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "copy:dev": "node run.cjs dev",
    "copy:prod": "node run.cjs prod",
    "watch:shared": "postcss ../shared/styles/index.pcss --output static/css/shared.css --watch --poll -u postcss-import autoprefixer --no-map",
    "start:dev": "concurrently \"npm run copy:dev && vite dev\" \"npm run watch:shared\"",
    "start:prod": "concurrently \"npm run copy:prod && vite dev\" \"npm run watch:shared\"",
    "build:dev": "npm run copy:dev && vite build",
    "build:prod": "npm run copy:prod && vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:integration": "playwright test",
    "test:unit": "vitest"
  },
  "devDependencies": {
    "@playwright/test": "1.46.1",
    "@sveltejs/adapter-static": "3.0.4",
    "@sveltejs/kit": "2.5.24",
    "@sveltejs/vite-plugin-svelte": "3.1.2",
    "@types/luxon": "3.4.2",
    "@typescript-eslint/eslint-plugin": "8.2.0",
    "@typescript-eslint/parser": "8.2.0",
    "autoprefixer": "10.4.20",
    "concurrently": "8.2.2",
    "eslint": "9.9.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-svelte": "2.43.0",
    "postcss": "8.4.41",
    "postcss-cli": "11.0.0",
    "postcss-import": "16.1.0",
    "svelte": "5.0.0-next.236",
    "svelte-check": "3.8.6",
    "tailwindcss": "3.4.10",
    "tslib": "2.7.0",
    "typescript": "5.5.4",
    "vite": "5.4.2",
    "vitest": "2.0.5"
  },
  "type": "module",
  "dependencies": {
    "@jaspero/modular": "1.28.0",
    "@jaspero/utils": "1.15.3",
    "@jaspero/web-components": "1.157.0",
    "deepcopy": "2.1.0",
    "dexie": "4.0.8",
    "firebase": "10.13.1",
    "grapesjs": "0.21.12",
    "grapesjs-parser-postcss": "1.0.3",
    "grapesjs-preset-newsletter": "1.0.2",
    "grapesjs-style-gradient": "3.0.3",
    "luxon": "3.5.0"
  }
}
