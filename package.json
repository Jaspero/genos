{
  "name": "webshop-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "lint": "prettier --check .",
    "format": "prettier --write . && npm run check --prefix cms && npm run check --prefix web && npm run lint:fix --prefix functions",
    "write:indexes": "firebase firestore:indexes > firestore.indexes.json",
    "configure-release": "tsx scripts/configure-release.ts",
    "send:test-email": "node scripts\\test-sendgrid-email.js",
    "send:test-email:service": "tsx scripts\\test-email-service.ts"
  },
  "type": "commonjs",
  "devDependencies": {
    "@types/luxon": "3.4.2",
    "node": "20.18.0",
    "prettier": "3.3.3",
    "prettier-plugin-svelte": "3.2.6",
    "tsx": "4.19.2"
  },
  "dependencies": {
    "@sendgrid/mail": "8.1.4",
    "firebase": "^11.0.2",
    "firebase-admin": "12.7.0",
    "luxon": "3.5.0"
  }
}
